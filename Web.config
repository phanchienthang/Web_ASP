			<add name="news_detail" virtualUrl="^~/tin-tuc/([\w-]*)-([0-9]*).html" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Modules/News/detail.aspx?idNews=$2" ignoreCase="true"/>
			<add name="news" virtualUrl="^~/tin-tuc.aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Modules/News/main.aspx" ignoreCase="true"/>
			<add name="about" virtualUrl="^~/ve-chung-toi.aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Modules/About/main.aspx" ignoreCase="true"/>
			<add name="contact" virtualUrl="^~/lien-he.aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Modules/Contact/main.aspx" ignoreCase="true"/>
			<add name="pagemain" virtualUrl="^~/thanhcong.aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Modules/Page/main.aspx" ignoreCase="true"/>
			<!--<add name="_pagemain" virtualUrl="^~/" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Modules/Page/main.aspx" ignoreCase="true"/>-->
		</rewrites>
